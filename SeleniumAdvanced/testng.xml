<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite thread-count="2" parallel="tests" name="LambdaTestSuite">
  <parameter name="url" value="https://www.lambdatest.com/" />
	<test name="Chrome + 86.0 + Windows 10">
    	<parameter name="browser" value="Chrome" />
		<parameter name="version" value="86.0" />
		<parameter name="platform" value="Windows 10" />
		<classes>
      		<class name="Tests.LambdaTest"/>
    	</classes>
  	</test>
	<test name="MicrosoftEdge + 87.0 + macOS Sierra">
		<parameter name="browser" value="MicrosoftEdge" />
		<parameter name="version" value="87.0" />
		<parameter name="platform" value="macOS Sierra" />
		<classes>
			<class name="Tests.LambdaTest"/>
		</classes>
	</test>
</suite> <!-- Suite -->
